   "summary": "Machine Learning for Many-Body Quantum Systems"
  },
  "build": {
   "noarch": "python",
   "number": "0",
   "script": [
    "PYTHON -m pip install --no-deps --ignore-installed --no-cache-dir -vvv ."
   ]
  },
  "extra": {
   "recipe-maintainers": [
    "PhilipVinc",
    "gcarleo"
   ]
  },
  "package": {
   "name": "netket",
   "version": "3.9.2"
  },
  "requirements": {
   "host": [
    "python >=3.8",
    "pip",
    "hatchling >=1.10",
    "hatch-vcs >=0.3"
   ],
   "run": [
    "python >=3.8",
    "numpy >=1.18",
    "scipy >=1.5.2",
    "python-igraph >=0.9",
    "jax >=0.3.16",
    "jaxlib >0.3.15",
    "flax >=0.6.0",
    "optax >=0.1.3",
    "mpi4py >=3.0.1",
    "mpi4jax >=0.3.1",
    "numba4jax >=0.0.9",
    "numba >=0.52.0",
    "orjson >=3.4",
    "tqdm >=4.56.2",
    "plum-dispatch >=1.5.1"
   ]
  },
  "source": {
   "sha256": "38cf7e9b631415b61b8d90554d32daeded3ca6df2fa050fceac216d1e707b162",
   "url": "https://pypi.io/packages/source/n/netket/netket-3.9.2.tar.gz"
  },
  "test": {
   "commands": [
    "",
    "pip install python-igraph",
    "pip check",
    "export OMPI_MCA_plm=isolated",
    "export OMPI_MCA_btl_vader_single_copy_mechanism=none",
    "export OMPI_MCA_rmaps_base_oversubscribe=yes",
    "export OMPI_ALLOW_RUN_AS_ROOT=1",
    "export OMPI_ALLOW_RUN_AS_ROOT_CONFIRM=1",
    "python -c \"import netket; print(netket.__version__)\""
   ],
   "requires": [
    "pip"
   ]
  }
 },
