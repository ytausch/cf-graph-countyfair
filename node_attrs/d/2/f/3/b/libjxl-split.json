 "linux_64_meta_yaml": {
  "about": {
   "description": "This repository contains a reference implementation of JPEG XL (encoder and decoder),\ncalled libjxl. This software library is used by many applications that support JPEG XL.\n\n`libjxl` provides the core library.\nFor `cjxl`, `djxl`, and other tools, use `libjxl-tools`.\n",
   "dev_url": "https://github.com/libjxl/libjxl",
   "home": "https://jpeg.org/jpegxl/",
   "license": "BSD-3-Clause",
   "license_family": "BSD",
   "license_file": [
    "LICENSE",
    "third_party/skcms/LICENSE"
   ],
   "summary": "JPEG XL image format reference implementation"
  },
  "build": {
   "number": "2"
  },
  "extra": {
   "feedstock-name": "libjxl",
   "recipe-maintainers": [
    "sshockwave"
   ]
  },
  "outputs": [
   {
    "build": {
     "ignore_run_exports": [
      "libbrotlicommon"
     ],
     "run_exports": [
      "libjxl"
     ]
    },
    "name": "libjxl",
    "requirements": {
     "build": [
      "c_compiler_stub",
      "cxx_compiler_stub",
      "pkg-config",
      "cmake >=3.15",
      "make"
     ],
     "host": [
      "brotli",
      "libhwy"
     ]
    },
    "script": "install.sh",
    "test": {
     "commands": [
      "test -f \"$PREFIX\"/include/jxl/encode.h",
      "test -f \"$PREFIX\"/lib/libjxl\"$SHLIB_EXT\"",
      "test ! -f \"$PREFIX\"/bin/cjxl",
      "test ! -f \"$PREFIX\"/lib/libjxl.a"
     ]
    }
   },
   {
    "build": {
     "ignore_run_exports": [
      "libbrotlicommon",
      "libbrotlidec"
     ]
    },
    "name": "libjxl-tools",
    "requirements": {
     "build": [
      "c_compiler_stub",
      "cxx_compiler_stub",
      "pkg-config",
      "cmake >=3.15",
      "make"
     ],
     "host": [
      "brotli",
      "libhwy",
      "giflib",
      "libjpeg-turbo",
      "openexr",
      "libpng",
      "libjxl"
     ],
     "run": [
      "libjxl"
     ]
    },
    "script": "install.sh",
    "test": {
     "commands": [
      "cjxl --version",
      "djxl --version",
      "jxlinfo --help",
      "printf \"P6 1 1 63 000\" > test.ppm",
      "cjxl test.ppm test.jxl",
      "djxl test.jxl test.ppm",
      "cjpeg_hdr test.ppm test.jpg",
      "jxlinfo test.jxl"
     ]
    }
   }
  ],
  "package": {
   "name": "libjxl-split",
   "version": "0.8.2"
  },
  "requirements": {
