     "build": [
      "meson",
      "ninja",
      "pkg-config",
      "c_compiler_stub",
      "cxx_compiler_stub",
      "ninja",
      "python >=2.7",
      "setuptools"
     ],
     "host": [
      "libglib",
      "zlib"
     ],
     "run": [
      "libglib"
     ]
    },
    "script": "install.sh",
    "test": {
     "commands": [
      "gapplication help",
