    "cpaulik",
    "mariusvniekerk",
    "wolfv"
   ]
  },
  "outputs": [
   {
    "build": {
     "run_exports": [
      "libnghttp2"
     ]
    },
    "files": [
     "include/nghttp2/nghttp2*",
     "lib/libnghttp2*",
     "lib/nghttp2*",
     "lib/pkgconfig/libnghttp2*",
     "bin/nghttp*"
    ],
    "name": "libnghttp2",
    "requirements": {
     "build": [
      "c_compiler_stub",
      "cxx_compiler_stub",
      "make",
      "pkg-config"
     ],
     "host": [
      "zlib",
      "openssl",
      "libev",
      "libev >=4.11",
      "c-ares",
      "c-ares >=1.7.5"
     ],
     "run": [
      "libstdcxx-ng",
      "libev"
     ]
    },
    "script": "install.sh",
    "test": {
     "commands": [
      "nghttp -nv https://nghttp2.org",
      "test ! -f ${PREFIX}/lib/libnghttp2.a",
      "test -f ${PREFIX}/lib/libnghttp2.so"
     ]
    }
   },
   {
    "build": null,
