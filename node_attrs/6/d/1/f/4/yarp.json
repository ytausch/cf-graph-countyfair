     ],
     "host": [
      "yarp-cxx",
      "python",
      "pip",
      "openssl"
     ],
     "run": [
      "yarp-cxx",
      "python",
      "numpy"
     ]
    },
    "script": "build_py.sh",
    "test": {
     "commands": [
      "test $(pip list | grep yarp | tr -s \" \" | grep $PKG_VERSION | wc -l) -eq 1",
